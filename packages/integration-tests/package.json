{
  "name": "@tui-sandbox/integration-tests",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "cy:open": "cypress open --e2e",
    "cy:run": "concurrently --success command-cypress --kill-others --names 'client,server,cypress' --prefix-colors 'blue,green,yellow' 'npm run dev:client' 'npm run dev:server' 'wait-on --timeout 60000 http://127.0.0.1:5173 && npx cypress run'",
    "dev": "concurrently --kill-others --names 'client,server,cypress' --prefix-colors 'blue,green,yellow' 'npm run dev:client' 'npm run dev:server' 'npm run cy:open'",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --watch library --watch ./MyTestDirectory.ts --ext '*' --exec 'npx tsx ./library/server/server.ts'",
    "eslint": "eslint --max-warnings=0 .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@catppuccin/palette": "1.3.0",
    "@runtyping/zod": "2.1.1",
    "@trpc/client": "11.0.0-rc.477",
    "@trpc/server": "11.0.0-rc.477",
    "@types/uuid": "10.0.0",
    "core-js": "3.38.1",
    "node-pty": "1.0.0",
    "tsx": "4.19.1",
    "wait-on": "8.0.1",
    "winston": "3.14.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "22.5.5",
    "@types/tinycolor2": "1.4.6",
    "@types/ws": "8.5.12",
    "@xterm/addon-attach": "0.11.0",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/xterm": "5.5.0",
    "concurrently": "9.0.1",
    "dree": "5.1.5",
    "express": "4.21.0",
    "nodemon": "3.1.4",
    "prettier-plugin-organize-imports": "4.0.0",
    "tinycolor2": "1.6.0",
    "typescript": "5.6.2",
    "vite": "5.4.5",
    "vitest": "2.1.1"
  }
}
