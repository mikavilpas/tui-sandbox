{
  "name": "@tui-sandbox/library",
  "version": "12.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikavilpas/tui-sandbox"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/client/index.d.ts",
      "import": "./dist/src/client/index.js"
    },
    "./browser/neovim-client.js": {
      "types": "./dist/src/browser/neovim-client.d.ts",
      "import": "./dist/src/browser/neovim-client.js"
    },
    "./client": {
      "types": "./dist/src/client/index.d.ts",
      "import": "./dist/src/client/index.js"
    },
    "./server": {
      "types": "./dist/src/server/index.d.ts",
      "import": "./dist/src/server/index.js"
    },
    "./dist/*": "./dist/*",
    "./src/*": "./dist/src/*"
  },
  "main": "./dist/src/client/index.js",
  "types": "./dist/src/client/index.d.ts",
  "bin": {
    "tui": "dist/src/scripts/tui.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "concurrently --names 'vite,tsc' 'vite build' 'tsc' --prefix-colors blue,green && attw --pack --profile=esm-only",
    "dev": "concurrently --names 'client,server' 'pnpm dev:client' 'pnpm dev:server' --prefix-colors blue,green",
    "dev:client": "vite",
    "dev:server": "cd ../integration-tests/ && nodemon --watch ../library --ext '*' --exec 'tsx ../library/src/scripts/tui.ts start'",
    "test": "vitest"
  },
  "dependencies": {
    "@catppuccin/palette": "1.7.1",
    "@trpc/client": "11.10.0",
    "@trpc/server": "11.10.0",
    "@xterm/addon-clipboard": "0.2.0",
    "@xterm/addon-fit": "0.11.0",
    "@xterm/addon-unicode11": "0.9.0",
    "@xterm/xterm": "6.0.0",
    "concurrently": "9.2.1",
    "cors": "2.8.6",
    "dree": "5.1.5",
    "express": "5.2.1",
    "neovim": "5.4.0",
    "node-pty": "1.0.0",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "winston": "3.19.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@types/command-exists": "1.2.3",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "24.10.13",
    "cypress": "15.10.0",
    "nodemon": "3.1.11",
    "type-fest": "5.4.4",
    "vite": "8.0.0-beta.13",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "cypress": "^13 || ^14 || ^15",
    "prettier": ">= 3.3.3",
    "type-fest": ">= 4.27.0",
    "typescript": ">= 5.6.3",
    "wait-on": ">=9.0.0",
    "zod": ">=4.0.0-0 <5"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
